<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:fontFamily="@font/poppins_bold"
        android:text="Subscription History"
        android:textColor="@color/purple_500"
        android:textSize="@dimen/_13ssp" />

    <LinearLayout
        android:id="@+id/llBillingPeriod"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView3"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/llTitles"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/rlSwitch"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:orientation="horizontal"
                android:weightSum="8">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_weight="0.5"
                    android:fontFamily="@font/poppins_bold"
                    android:gravity="start"
                    android:text="Sr. No"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_weight="2.5"
                    android:fontFamily="@font/poppins_bold"
                    android:gravity="center"
                    android:text="Subscription Plan"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_bold"
                    android:gravity="center"
                    android:text="Start Date"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_bold"
                    android:gravity="center"
                    android:text="End Date"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_bold"
                    android:gravity="center"
                    android:text="Status"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:background="#330F0A44" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llPrices"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/rlSwitch"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:orientation="horizontal"
                android:weightSum="8">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_weight="0.5"
                    android:fontFamily="@font/poppins_medium"
                    android:gravity="start"
                    android:text="1"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_weight="2.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="start"
                    android:text="Daily Regular Water Plan"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_10ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="center"
                    android:text="1/1/21"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="center"
                    android:text="1/1/22"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="center"
                    android:text="Complete"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_10ssp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:orientation="horizontal"
                android:weightSum="8">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_weight="0.5"
                    android:fontFamily="@font/poppins_medium"
                    android:gravity="start"
                    android:text="2"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_weight="2.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="start"
                    android:text="Daily Chilled Water Plan"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_10ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="center"
                    android:text="1/1/21"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="center"
                    android:text="1/1/22"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="center"
                    android:text="Running"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_10ssp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:orientation="horizontal"
                android:weightSum="8">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_weight="0.5"
                    android:fontFamily="@font/poppins_medium"
                    android:gravity="start"
                    android:text="3"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/_2sdp"
                    android:layout_weight="2.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="start"
                    android:text="Flexible Chilled Water Plan"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_10ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="center"
                    android:text="1/1/21"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="center"
                    android:text="1/1/22"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_11ssp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.5"
                    android:fontFamily="@font/poppins_regular"
                    android:gravity="center"
                    android:text="Running"
                    android:textColor="@color/purple_500"
                    android:textSize="@dimen/_10ssp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:background="#330F0A44" />

        </LinearLayout>

    </LinearLayout>

</RelativeLayout>